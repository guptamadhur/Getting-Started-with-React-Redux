{
  "name": "react-redux",
  "description": "Getting started with React Redux",
  "author": "Madhur Gupta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptamadhur/Getting-Started-with-React-Redux.git"
  },
  "scripts": {
    "start": "run-p start:dev start:api",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 3100",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "poststart": "nodemon tools/apiExpressServer.js",
    "server": "node tools/newapiServer.js"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "express": "^4.17.1",
    "immer": "^7.0.8",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.0.0",
    "react-toastify": "^6.0.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "fetch-mock": "^9.10.7",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "jest": "^26.4.2",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.11.0",
    "node-fetch": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^8.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
